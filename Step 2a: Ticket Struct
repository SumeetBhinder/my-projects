#include <iostream>
#include "Ticket.h"
#include "Show.h"

int main() {
    // Create Show instance
    Show myShow;
    myShow.setTitle("Hamlet");
    myShow.setVenue("Grand Theater");
    myShow.setShowTime(2000);

    // Create Ticket instance
    Ticket myTicket;
    myTicket.section = "Orchestra";
    myTicket.row = 5;
    myTicket.seat = 12;
    myTicket.price = 120.00;

    // Print ticket info (function provided in starter project)
    printTicketInfo(myShow, myTicket);

    return 0;
}
